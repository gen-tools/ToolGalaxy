# Production Environment Variables for Firebase Hosting
# Copy this file and update values for your production environment

# API Configuration
VITE_API_URL=https://toolgalaxy-app.web.app/api
VITE_APP_URL=https://toolgalaxy-app.web.app

# Firebase Configuration (if using Firebase services)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=toolgalaxy-app.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=toolgalaxy-app
VITE_FIREBASE_STORAGE_BUCKET=toolgalaxy-app.appspot.com

# Analytics and Tracking
VITE_GA_TRACKING_ID=your_google_analytics_id
VITE_GTM_ID=your_google_tag_manager_id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PWA=true

# Builder.io (if using)
VITE_PUBLIC_BUILDER_KEY=your_builder_public_key

# Performance Monitoring
VITE_SENTRY_DSN=your_sentry_dsn
VITE_VERSION=1.0.0
